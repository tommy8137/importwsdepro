{
  "name": "koa_project",
  "version": "1.0.0",
  "description": "koa_test",
  "main": "index.js",
  "scripts": {
    "generateToken": "node ./test/api/generateToken.js",
    "prd": "node --max-old-space-size=1750 --expose-gc server.js",
    "sit": "node --max-old-space-size=1750 --expose-gc server.js",
    "sit-pm2": "pm2 kill && pm2-runtime start -i 3 server.js --name=procure_web --node-args='--max-old-space-size=1750 --expose-gc'",
    "dev": "nodemon --max-old-space-size=1750 --expose-gc server.js",
    "dev-pm2": "pm2 kill && pm2-runtime start -i 3 server.js --name=procure_web --node-args='--max-old-space-size=1750 --expose-gc'",
    "apidoc": "apidoc -i server/router/ -o apidoc/",
    "testapi": "mocha --slow 600  --expose-gc --recursive ./test/api/**/*.test.js --timeout 60000 --exit",
    "testunit": "mocha --slow 600 --expose-gc  --recursive ./test/unit --exit",
    "testunitwithcoverage": "nyc --reporter=text --reporter=lcov --reporter=html --all mocha --expose-gc --timeout 5000 --slow 600  --recursive ./test/unit --exit",
    "testunitwithHtmlcoverage": "nyc --reporter=html --all mocha --expose-gc --slow 600  --recursive ./test/unit --exit",
    "createtestbom": "node ./test/intergration/testMod/testBom c",
    "removetestbom": "node ./test/intergration/testMod/testBom rm",
    "testbom": "mocha ./test/intergration/testBom/*.test.js --reporter mochawesome",
    "eslintReport": "eslint . -f node_modules/eslint-html-reporter/reporter.js -o ./eslint-report.html"
  },
  "author": "",
  "license": "ISC",
  "nyc": {
    "include": [
      "server"
    ],
    "exclude": [
      "server/router",
      "server/api",
      "server/model",
      "server/helpers/database"
    ]
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-html-reporter": "^0.7.4",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "mochawesome": "^4.1.0",
    "nodemon": "1.18.7",
    "nyc": "^13.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "7.1.1",
    "supertest": "^3.3.0"
  },
  "dependencies": {
    "activedirectory": "0.7.2",
    "apidoc": "^0.17.6",
