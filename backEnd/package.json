    "async-mkdirp": "^1.2.7",
    "axios": "^0.18.0",
    "babel-eslint": "9.0.0",
    "bluebird": "^3.5.5",
    "co": "4.6.0",
    "core-js": "^3.6.5",
    "db-migrate": "^0.11.5",
    "db-migrate-pg": "0.5.0",
    "eslint": "5.6.0",
    "eslint-plugin-react": "7.11.1",
    "excel-column-name": "^1.0.0",
    "exceljs": "^4.1.1",
    "fast-csv": "^2.4.1",
    "global": "^4.3.2",
    "jest": "23.6.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "8.3.0",
    "jszip": "^3.4.0",
    "koa": "2.5.3",
    "koa-body": "^4.0.4",
